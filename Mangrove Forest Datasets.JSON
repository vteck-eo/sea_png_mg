// Visualization
var visProb = {
  min: 0,
  max: 100,
  palette: ['white', 'yellow', 'orange', 'red', 'purple']
};

// -----------------------------------------------------
// Load probability mosaics (country-level assets)
// -----------------------------------------------------
var probabilityAssets = [
  "projects/servir-mekong/embedd/sea_img_brunei/2024",
  "projects/servir-mekong/embedd/sea_img_indo/2024",
  "projects/servir-mekong/embedd/sea_img_mls/2024",
  "projects/servir-mekong/embedd/sea_img_mym/2024",
  "projects/servir-mekong/embedd/sea_img_phil/2024",
  "projects/servir-mekong/embedd/sea_img_png/2024",
  "projects/servir-mekong/embedd/sea_img_sg/2024",
  "projects/servir-mekong/embedd/sea_img_thl/2024",
  "projects/servir-mekong/embedd/sea_img_timor_v1/2024",
  "projects/servir-mekong/embedd/sea_img_vnm/2024",
  "projects/servir-mekong/embedd/sea_khm_img/2024"
];

// Convert to ImageCollection
var probIC_2024 = ee.ImageCollection(
  probabilityAssets.map(function(path) {
    return ee.Image(path)
      .rename('mangrove_probability')
      .toFloat();
  })
);

// Mosaic into regional map
var probSEA_2024 = probIC_2024.mosaic();

// Add to map
Map.addLayer(probSEA_2024, visProb, 'Mangrove Probability 2024');
Map.setCenter(104.937, 12.207,6)
